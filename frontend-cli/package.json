{
  "name": "avangcli-frontend",
  "version": "1.0.0",
  "description": "Frontend CLI for scaffolding Next.js projects",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "commit": "cz",
    "dev": "node index.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "type-check:strict": "tsc --noEmit -p tsconfig.strict.json",
    "lint": "cd .. && eslint frontend-cli/",
    "lint:fix": "cd .. && eslint frontend-cli/ --fix",
    "format": "cd .. && prettier --write \"frontend-cli/**/*.{js,ts,json,md}\"",
    "format:check": "cd .. && prettier --check \"frontend-cli/**/*.{js,ts,json,md}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cli",
    "nextjs",
    "scaffolding",
    "frontend"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/yargs": "^17.0.32",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "typescript": "^5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
